<Window x:Class="secondPr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:secondPr"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Button x:Name="fp" Grid.Column="0" Height="40" VerticalAlignment="Top" Content="Page 1" Click="fp_Click"/>
        <Button x:Name="sp" Grid.Column="1" Height="40" VerticalAlignment="Top" Content="Page 2" Click="sp_Click"/>
        <Frame x:Name="NUF" Grid.Column="0" Grid.ColumnSpan="2" Height="300" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
